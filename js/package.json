{
  "name": "slipbox-js",
  "version": "0.0.0",
  "description": "Slipbox JS",
  "main": "dist/slipbox.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "check": "npm run format && npm run lint && npm run typecheck",
    "check:fix": "npm run format:fix && npm run lint:fix && npm run typecheck",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint": "eslint scripts src test",
    "lint:fix": "eslint scripts src test --fix",
    "test": "nyc mocha",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lggruspe/slipbox.git"
  },
  "keywords": [
    "slipbox"
  ],
  "author": "Levi Gruspe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lggruspe/slipbox/issues"
  },
  "homepage": "https://github.com/lggruspe/slipbox#readme",
  "devDependencies": {
    "@types/cytoscape": "^3.19.9",
    "@types/flexsearch": "^0.7.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "esbuild": "^0.13.8",
    "eslint": "^8.44.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.0.0-beta.88",
    "boxicons": "^2.1.4",
    "cytoscape": "^3.23.0",
    "flexsearch": "^0.7.21",
    "mathjax": "^3.2.2"
  }
